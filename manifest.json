{
  "manifest_version": 3,
  "name": "SPY LINK - Surendran | SEO Metrics Checker",
  "version": "2.0",
  "description": "SPY LINK is a fast and lightweight SEO tool that gives instant access to key domain metrics like DA, PA, Backlinks, Spam Score, and Domain Age â€” all in one click.",
  "key": "",
  "update_url": "https://clients2.google.com/service/update2/crx",
  "permissions": ["activeTab", "storage", "tabs"],
  "host_permissions": [
    "https://jsonwhoisapi.com/*",
    "https://api.whois.vu/*"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_icon": {
      "16": "icons/16.png",
      "32": "icons/32.png",
      "48": "icons/48.png",
      "128": "icons/128.png"
    },
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "icons/16.png",
    "32": "icons/32.png",
    "48": "icons/48.png",
    "128": "icons/128.png"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"],
      "run_at": "document_idle"
    }
  ]
}
